  "scripts": {
    "init": "heroku git:remote -a my-app -r prod && heroku git:remote -a my-app-staging -r staging",
    "preversion": "npm run build && git add build",
    "start": "node build/server.js",
    "start:prod": "NODE_ENV=production time npm run --silent build && date && NODE_ENV=production npm run --silent start",
    "build": "rimraf build && NODE_ENV=production webpack --config webpack.web.js",
    "build:browser": "webpack --config webpack.browser.js",
    "build:server": "webpack --config webpack.server.js",
    "deploy:prod": "heroku pipelines:promote -a my-app-staging",
    "lsp": "heroku releases -a my-app",
    "lss": "heroku releases -a my-app-staging",
    "dev": "DEBUG=my-app* NODE_ENV=development nodemon -x node server/dev.index.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint --cache flow src server",
    "lint:flow": "npm run --silent lint && npm run --silent flow",
    "test": "concurrently 'npm run --silent lint' 'npm run --silent flow' && npm run --silent test:tape | faucet",
    "test:tape": "NODE_ENV=test babel-node test.js",
    "watch": "nodemon -i .eslintcache -x npm run --silent test",
    "watch:lint": "nodemon -i .eslintcache -x npm run --silent lint:flow",
    "watch:test": "nodemon -i .eslintcache -x npm run --silent test:tape"
  }
}
